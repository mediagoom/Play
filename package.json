{
  "name": "mgplay",
  "version": "0.0.3",
  "description": "The mg player",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch-js & npm run serve",
    "watch-js": "watchify -d -p [ browserify-livereload --host 127.0.0.1 --port 1337 ] -t [ babelify --presets [ es2015 ] ] src/main.js -o src/bundle.js -v",
    "serve": "http-server -o",
    "karma": "node ./node_modules/karma/bin/karma start",
    "test": "node ./node_modules/karma/bin/karma start --single-run --no-auto-watch",
    "localtest": "node ./node_modules/karma/bin/karma start --single-run --no-auto-watch --testuri=http://localhost/video/002569741225_Sintel_2010_1080p_mkv/STATIC",
    "souce": "node ./node_modules/karma/bin/karma start ./karma.conf.soucelabs.js",
    "package": "browserify src/play.js --standalone mgplay --debug -t [ babelify --presets [ es2015 ] ] | exorcist index.js.map > index.js",
    "bundle": "browserify -t [ babelify ] src/main.js -o src/bundle.js -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mediagoom/play.git"
  },
  "keywords": [
    "upload",
    "chunk"
  ],
  "author": "admin@mediagoom.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mediagoom/play/issues"
  },
  "homepage": "https://github.com/mediagoom/play#readme",
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/helper-module-imports": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-transform-object-assign": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-cli": "^6.26.0",
    "chai": "^3.5.0",
    "exorcist": "^1.0.1",
    "http-server": "^0.9.0",
    "karma": "^2.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-edge-launcher": "^0.4.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.2.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "watchify": "^3.7.0",
    "babelify": "^7.3.0",
    "browserify": "^16.2.2",
    "browserify-livereload": "^1.0.8"
  },
  "dependencies": {
    "dashjs": "2.4.1",
    "debug": "^3.1.0",
    "urijs": "^1.18.9"
  }
}
